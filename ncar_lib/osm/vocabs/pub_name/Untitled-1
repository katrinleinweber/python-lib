		<id>SOARS-000-000-000-055</id> 
		
			
			
				
			
		
		<xmlFormat>osm</xmlFormat>  			
		<collection recordId="NCAR-COLLECTION-000-000-000-017" ky="soars" key="soars">Manuscripts - SOARS Papers</collection>
		<fileLastModified>2010-11-21T01:49:08Z</fileLastModified> 
		
		
		
			
						
			
				
		
